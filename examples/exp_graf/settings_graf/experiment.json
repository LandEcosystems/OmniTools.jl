{
  "basics": {
    "config_files": {
      "forcing": "forcing.json",
      "model_structure": "model_structure.json",
      "optimization": "optimization.json"
    },
    "domain": "Africa",
    "name": "OptimDomain",
    "time": {
      "date_begin": "2006-01-01",
      "date_end": "2013-12-31",
      "temporal_resolution": "day"
    }
  },
  "exe_rules": {
    "input_array_type": "keyed_array",
    "input_data_backend": "netcdf",
    "land_output_type": "array",
    "longtuple_size": null,
    "model_array_type": "static_array",
    "model_number_type": "Float64",
    "parallelization": "threads",
    "tolerance": 1.0e-2,
    "yax_max_cache": 2e9
  },
  "flags": {
    "calc_cost": true,
    "catch_model_errors": false,
    "debug_model": false,
    "filter_nan_pixels": false,
    "inline_update": false,
    "run_forward": true,
    "run_optimization": false,
    "save_info": true,
    "spinup_TEM": true,
    "store_spinup": false,
    "use_forward_diff": false
  },
  "model_output": {
    "depth_dimensions": {
      "d_cEco": 8,
      "d_snow": "snowW",
      "d_soil": "soilW"
    },
    "format": "nc",
    "output_array_type": "array",
    "path": null,
    "plot_model_output": false,
    "save_single_file": false,
    "variables": {
      "fluxes.evapotranspiration": null,
      "fluxes.gpp": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "diagnostics.gpp_f_airT": null,
      "diagnostics.gpp_demand": null,
      "diagnostics.gpp_f_climate": null,
      "diagnostics.gpp_potential": null,
      "diagnostics.gpp_f_vpd": null,
      "pools.soilW": "d_soil",
      "states.fAPAR": null,
      "states.PAW": "d_soil",
      "diagnostics.water_balance": null,
      "diagnostics.WUE": null
    }
  },
  "model_spinup": {
    "restart_file": null,
    "sequence": [
      {
        "forcing": "all_years",
        "n_repeat": 1,
        "spinup_mode": "all_forward_models"
      }
    ]
  }
}
